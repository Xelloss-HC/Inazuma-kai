{
   "background": {
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "32": "icon.png"
      },
      "default_popup": "popup.html",
      "default_title": "bahamut-warner"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "bower_components/jquery/dist/jquery.js", "scripts/extra.js", "scripts/contentscript3.js" ],
      "matches": [ "*://game.granbluefantasy.jp/*", "http://gbf.game.mbga.jp/*" ],
      "run_at": "document_end"
   } ],
   "description": "Will notify whenever raid shows in the raid page",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Grandblue Fantasy Bahamut Warner",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "notifications", "storage" ],
   "version": "0.0.4",
   "web_accessible_resources": [ "reload.js", "inject/click_.js", "inject/load_.js", "inject/engine.js", "inject/select_all_normal.js", "inject/weapon.js" ]
}
